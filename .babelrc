{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "useBuiltIns": true
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ]
}